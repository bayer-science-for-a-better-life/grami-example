{
  "person": {
    "dataPath": "src/main/resources/phone-calls/person.csv",
    "separator": ",",
    "processor": "person",
    "batchSize": 2000,
    "threads": 4,
    "attributes": [
      {
        "columnName": "first_name",
        "generator": "first-name"
      },
      {
        "columnName": "last_name",
        "generator" : "last-name"
      },
      {
        "columnName": "phone_number",
        "generator": "phone-number"
      },
      {
        "columnName": "city",
        "generator": "city"
      },
      {
        "columnName": "age",
        "generator": "age"
      },
      {
        "columnName": "nick_name",
        "generator": "nick-name",
        "listSeparator": ";"
      }]
  },
  "company": {
    "dataPath": "src/main/resources/phone-calls/company.csv",
    "separator": ",",
    "processor": "company",
    "attributes": [
      {
        "columnName": "name",
        "generator": "name"
      }],
    "batchSize": 100,
    "threads": 1
  },
  "contract": {
    "dataPath": "src/main/resources/phone-calls/contract.csv",
    "separator": ",",
    "processor": "contract",
    "players": [
      {
        "columnName": "company_name",
        "generator": "provider"
      },
      {
        "columnName": "person_id",
        "generator": "customer",
        "listSeparator": "###"
      }
    ],
    "batchSize": 100,
    "threads": 4
  },
  "calls": {
    "dataPath": "src/main/resources/phone-calls/call.csv",
    "separator": ",",
    "processor": "call",
    "players": [
      {
        "columnName": "caller_id",
        "generator": "caller"
      },
      {
        "columnName": "callee_id",
        "generator": "callee"
      }
    ],
    "attributes": [
      {
        "columnName": "started_at",
        "generator": "started-at"
      },
      {
        "columnName": "duration",
        "generator" : "duration"
      }
    ],
    "batchSize": 100,
    "threads": 4
  },
  "communication-channel": {
    "dataPath": "src/main/resources/phone-calls/communication-channel.csv",
    "separator": ",",
    "processor": "communication-channel",
    "batchSize": 100,
    "threads": 4,
    "players": [
      {
        "columnName": "peer_1",
        "generator": "peer"
      },
      {
        "columnName": "peer_2",
        "generator": "peer"
      }
    ],
    "relationPlayers": [
      {
        "columnName": "call_started_at",
        "generator": "past-call",
        "matchByAttribute": "started-at",
        "listSeparator": "###"
      }
    ]
  },
  "communication-channel-pm": {
    "dataPath": "src/main/resources/phone-calls/communication-channel-pm.csv",
    "separator": ",",
    "processor": "communication-channel",
    "batchSize": 100,
    "threads": 4,
    "players": [
      {
        "columnName": "peer_1",
        "generator": "peer"
      },
      {
        "columnName": "peer_2",
        "generator": "peer"
      }
    ],
    "relationPlayers": [
      {
        "columnNames": ["peer_1", "peer_2"],
        "generator": "past-call",
        "matchByPlayers": ["caller", "callee"]
      }
    ]
  },
  "append-twitter": {
    "dataPath": "src/main/resources/phone-calls/append-twitter.csv",
    "separator": ",",
    "processor": "append-twitter-to-person",
    "attributes": [
      {
        "columnName": "phone_number",
        "generator": "phone-number",
        "match": true
      },
      {
        "columnName": "twitter",
        "generator": "twitter-username",
        "listSeparator": "###"
      }
    ],
    "batchSize": 100,
    "threads": 4
  },
  "append-call-rating": {
    "dataPath": "src/main/resources/phone-calls/append-call-rating.csv",
    "separator": ",",
    "processor": "append-call-rating-to-call",
    "attributes": [
      {
        "columnName": "started_at",
        "generator": "started-at",
        "match": true
      },
      {
        "columnName": "call_rating",
        "generator": "call-rating"
      }
    ],
    "batchSize": 100,
    "threads": 4
  },
  "append-pp-fakebook": {
    "dataPath": "src/main/resources/phone-calls/append-fb-preprocessed.csv",
    "separator": ",",
    "processor": "append-pp-fb-to-person",
    "attributes": [
      {
        "columnName": "phone_number",
        "generator": "phone-number",
        "match": true
      },
      {
        "columnName": "fb",
        "generator": "fakebook-link",
        "listSeparator": "###",
        "preprocessor": {
          "type": "regex",
          "params": {
            "regexMatch" : "^.*(fakebook\\.com.*)/$",
            "regexReplace": "$1"
          }
        }
      }
    ],
    "batchSize": 100,
    "threads": 4
  }
}